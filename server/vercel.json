{
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/todo/(.*)", 
        "dest": "/todo/"   
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  